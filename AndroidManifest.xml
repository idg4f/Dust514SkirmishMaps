<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.skink.dustmapactivity"
    android:versionCode="001"
    android:versionName="0.0.1" >

    <uses-sdk
        android:minSdkVersion="4"
        android:targetSdkVersion="15" />

    <application
        android:hardwareAccelerated="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity android:name="net.skink.dustmapactivity.LauncherActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="net.skink.dustmapactivity.SimpleSampleActivity" />
        <activity android:name="net.skink.dustmapactivity.ViewPagerActivity" />
        <activity android:name="net.skink.dustmapactivity.AshlandActivity" />
        <activity android:name="net.skink.dustmapactivity.IronDeltaActivity" />
        <activity android:name="net.skink.dustmapactivity.LineHarvestActivity" />
        <activity android:name="net.skink.dustmapactivity.ManusPeakActivity" />
        <activity android:name="net.skink.dustmapactivity.SkimJunctionActivity" />
        <activity android:name="net.skink.dustmapactivity.SpineCrescentActivity" />
    </application>

</manifest>